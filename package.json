{
  "name": "anatomogram",
  "version": "2.1.1",
  "description": "Anatomogram component for Expression Atlas heatmap",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "parseSvgs": "node scripts/svgParse.js src/svg > src/json/svgsMetadata.json",
    "prepack": "npm run parseSvgs && rm -rf lib && babel src -d lib --copy-files",
    "dist": "npm run parseSvgs && webpack -p",
    "pretest": "npm run parseSvgs",
    "test": "jest --coverage",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme"
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "contributors": [
    "Wojciech Bazant <wbazant@ebi.ac.uk>",
    "Andrew Diamantoukos <andrewd@jimmy.harvard.edu>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/anatomogram.git"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-svg": "^7.2.7",
    "recompose": "^0.30.0",
    "styled-components": "^4.1.3",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-gene-expression": "^0.4.2",
    "fast-xml-parser": "^3.12.13",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "react-test-renderer": "^16.8.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
